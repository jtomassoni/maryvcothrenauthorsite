  "description": "Mary's Author Website - Simple Email Collection Site",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:api\" \"vite\"",
    "dev:api": "node server.js",
    "dev:frontend": "vite",
    "build": "vite build",
    "db:generate": "prisma generate",
    "db:migrate": "node scripts/db-migrate.js",
    "db:push": "node scripts/db-push.js",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dompurify": "^3.0.6",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "marked": "^11.1.1",
    "nodemailer": "^6.9.7",
    "resend": "^6.1.0",
    "vue": "^3.4.0",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.10",
    "@types/cookie-parser": "^1.4.6",
    "@types/dompurify": "^3.0.5",
    "@types/marked": "^6.0.0",
    "@types/node": "^20.10.0",
    "@types/nodemailer": "^6.4.14",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "@vitejs/plugin-vue": "^4.5.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.4.0",
    "autoprefixer": "^10.4.16",
    "concurrently": "^9.2.1",
    "dotenv-cli": "^11.0.0",
    "eslint": "^8.54.0",
    "eslint-plugin-vue": "^9.18.0",
    "minimatch": "^3.1.2",
    "postcss": "^8.4.32",
    "prettier": "^3.1.0",
    "prisma": "^5.7.1",
    "tailwindcss": "^3.3.6",
    "typescript": "~5.3.0",
    "vite": "^5.0.0",
    "vue-tsc": "^1.8.25"
