# Database (required)
# PostgreSQL connection string for Prisma
DATABASE_URL=postgresql://user:password@host:port/database?sslmode=require

# Authentication (required)
# Admin login credentials
AUTH_USERNAME=your_admin_username
AUTH_PASSWORD=your_admin_password

# Session signing secret (required for server.js, optional in dev)
# Used by lib/auth.js for HMAC-based session tokens
# Has dev fallback, but REQUIRED in production
AUTH_SECRET=your_random_secret_key_here_min_32_chars


